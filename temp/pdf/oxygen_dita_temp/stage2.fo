                    <fo:block space-before="0.6em" space-after="0.6em" id="unique_23_Connect_42_Supported_chain_and_rules_on-the_IBM_StoredIQ_gateway">
                        <fo:block space-before="0.6em" space-after="0.6em" text-indent="0em"><fo:inline font-weight="bold">Supported chain and rules on the IBM StoredIQ gateway</fo:inline></fo:block>
                        <fo:block space-before="0.6em" space-after="0.6em" text-indent="0em">In iptables, the following firewall and chain rules are
                            defined:</fo:block>
                        <fo:block font-size="10pt" space-before="0.6em" space-after="0.6em" white-space-treatment="preserve" white-space-collapse="false" linefeed-treatment="preserve" wrap-option="wrap" font-family="monospace" line-height="106%" keep-with-previous.within-page="always" start-indent="6pt + from-parent(start-indent)" end-indent="6pt + from-parent(end-indent)" padding="6pt" background-color="#f0f0f0" hyphenation-character="►">
                            'PROD-transport':['5434','8765','7766','11102','11103','11104'],
                            'PROD-https':['443'],
                            'PROD-ssh':['22'],
                            'PROD-web':['80'],
                            'PROD-postgres':[’5432’]
                            
                        </fo:block>
                        <fo:block font-size="10pt" space-before="0.6em" space-after="0.6em" white-space-treatment="preserve" white-space-collapse="false" linefeed-treatment="preserve" wrap-option="wrap" font-family="monospace" line-height="106%" keep-with-previous.within-page="always" start-indent="6pt + from-parent(start-indent)" end-indent="6pt + from-parent(end-indent)" padding="6pt" background-color="#f0f0f0" hyphenation-character="►">
  'desktop' service:
                            'PROD-broker':['21000'],
                            'PROD-collectionsvc':['21300:21399'],
                            'PROD-desktopupgrade':['21004'],
                            'PROD-objlistmgr':['21100:21199'],
                            'PROD-objlistsvc':['21200:21299'],
                            'PROD-registration':['21001'],
                            'PROD-session':['21002'],
                            'PROD-task':['21003'],</fo:block>
